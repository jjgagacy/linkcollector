clear lock
clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5
!Super_L to Control_L
keycode 133 = Control_L
!Caps_Lock to Escape
keycode 66 = Escape
!Super_R to Home
keycode 134 = Home
!Alt_R to End
keycode 108 = End
add control = Control_L Control_R
add mod1 = Alt_L Alt_R Meta_L
add mod2 = Num_Lock
add mod3 = Hyper_L
add mod4 = Super_L Super_R
add mod5 = Mode_switch ISO_Level3_Shift
